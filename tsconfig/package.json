{
  "name": "testinginign",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.0.36",
    "@types/node": "^8.0.13",
    "@types/socket.io": "^1.4.29",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "^6.23.0",
    "babel-register": "^6.24.1",
    "eslint": "^4.2.0",
    "eslint-config-esayemm": "^2.0.4",
    "eslint-plugin-react": "^7.1.0",
    "expect": "^1.20.2",
    "jbs-node": "^0.0.3",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "ts-node": "^3.2.0",
    "typescript": "^2.4.1"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-register --require babel-polyfill $(find . -name '*.spec.js' ! -ipath '*node_modules*')",
    "test:watch": "NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-register --require babel-polyfill --watch $(find . -name '*.spec.js' ! -ipath '*node_modules*')",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "build": "./node_modules/jbs-node/bin.js build --input src --output build",
    "prepublish": "npm run build",
    "preversion": "npm run lint && npm run test",
    "version": "npm publish",
    "postversion": "git add . && git push && git push --tags",
    "start": "npm run build:live",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./index.ts"
  },
  "main": "./build/index.js",
  "babel": {
    "presets": [
      "./node_modules/jbs-node/configs/babel-preset-jbs-node.js"
    ]
  },
  "files": [
    "build/"
  ]
}
